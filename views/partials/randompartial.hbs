<div class="card-deck" style="width: 18rem;">
    <div class="card">
        <img class="card-img-lef" src="{{this.image_url}}" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">{{this.description}}</p>
            <p class="card-text">{{this.food_pairing}}</p>
            <p class="card-text"><small class="text-muted">{{this.tagline}}</small></p>
        </div>
    </div>
</div>